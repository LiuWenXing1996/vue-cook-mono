    ".": {
      "import": "./dist/runtime.js",
      "require": "./dist/runtime.cjs",
      "types": "./dist/runtime.d.ts"
    },
    "./design": {
      "import": "./dist/design.js",
      "require": "./dist/design.cjs",
      "types": "./dist/design.d.ts"
