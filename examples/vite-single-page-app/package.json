{
  "name": "vite-single-page-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "build:deps": "vue-cook build-deps -c ./cook.config.json -p ./package.json -d ./",
    "build:schema": "vue-cook build-schema -c ./cook.config.json -p ./package.json -d ./",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false"
  },
  "dependencies": {
    "@vue-cook/core": "workspace:^0.0.1",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.0",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.3.1",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vue-cook/cli": "workspace:^0.0.1",
    "@vue/tsconfig": "^0.4.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-node-polyfills": "^0.14.1",
    "vue-tsc": "^1.8.8"
  }
}
