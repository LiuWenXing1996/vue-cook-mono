{
  "extends": "./tsconfig.json",
  "include": [
    "env.d.ts",
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue"
  ],
  "compilerOptions": {
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
}