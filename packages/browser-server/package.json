{
  "name": "@vue-cook/browser-server",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite build -w",
    "build": "run-p type-check \"build-only {@}\" --",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vue-cook/core": "workspace:^0.0.1",
    "mime": "^3.0.0",
    "process": "^0.11.10"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/mime": "^3.0.1",
    "@types/node": "^18.17.17",
    "eslint": "^8.49.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.4"
  }
}
