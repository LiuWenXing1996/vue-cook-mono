{
    "root": "./src",
    "ignorePaths": [
        "node_modules",
        "dist",
        "public/lowcode"
    ],
    "minify": true,
    "sourcemap": true,
    "outdir": "./dist",
    "renderer": {
        "runtime": {
            "packageName": "@vue-cook/render",
            "varName": "RuntimeRenderer"
        },
        "design": {
            "packageName": "@vue-cook/render",
            "varName": "DesignRenderer"
        },
        "editor": {
            "packageName": "@vue-cook/render",
            "varName": "EditorRenderer"
        }
    },
    "embed": [
        {
            "name": "vue-eco-system",
            "externals": [
                {
                    "packageName": "vue",
                    "injectName": "Vue"
                },
                {
                    "packageName": "vue-router",
                    "injectName": "VueRouter"
                },
                {
                    "packageName": "@vue-cook/element-plus-materials",
                    "injectName": "ElementPlusMaterials"
                }
            ]
        }
    ]
}