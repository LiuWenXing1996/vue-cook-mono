{
    "root": "./src",
    "ignorePaths": [
        "node_modules",
        "dist",
        "public/lowcode"
    ],
    "minify": false,
    "sourcemap": true,
    "outdir": "./dist",
    "embed": [
        {
            "name": "vue-eco-system",
            "externals": [
                {
                    "packageName": "vue",
                    "injectName": "Vue"
                },
                {
                    "packageName": "vue-router",
                    "injectName": "VueRouter"
                },
                {
                    "packageName": "@vue-cook/element-plus-materials",
                    "injectName": "ElementPlusMaterials"
                }
                
            ]
        }
    ]
}